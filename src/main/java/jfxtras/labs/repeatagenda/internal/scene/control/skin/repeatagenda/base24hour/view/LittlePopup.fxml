<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane stylesheets="@main.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jfxtras.labs.repeatagenda.internal.scene.control.skin.agenda.base24hour.controller.LittlePopupController">
   <children>
      <VBox fx:id="vBox" layoutY="7.0" spacing="5.0" styleClass="simple-border" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0">
         <children>
            <Label text="manage appointment">
               <styleClass>
                  <String fx:value="bordered-titled-title" />
                  <String fx:value="background3" />
               </styleClass>
            </Label>
            <HBox alignment="CENTER_LEFT" spacing="5.0">
               <children>
                  <Label text="Name" />
                  <TextField fx:id="nameTextField" HBox.hgrow="ALWAYS" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <Label fx:id="appointmentTimeLabel" text="Label" />
            <HBox spacing="5.0">
               <children>
                  <HBox HBox.hgrow="ALWAYS" />
                  <Button fx:id="editAppointmentButton" mnemonicParsing="false" onAction="#handleEditAppointment" prefWidth="93.0" text="Edit" />
                  <Button fx:id="deleteAppointmentButton" mnemonicParsing="false" onAction="#handleDeleteAppointment" prefWidth="93.0" text="Delete" />
                  <HBox layoutX="10.0" layoutY="10.0" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </children>
   <styleClass>
      <String fx:value="background3" />
      <String fx:value="AgendaPopup" />
   </styleClass>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
